<UserControl x:Class="Jacobi.Vst.Samples.CorePlugin.EditorControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="300" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition />
        </Grid.RowDefinitions>
        
        <ListBox Grid.Column="0" Name="ListBox">
            
        </ListBox>
        
        <Button Name="OpenBtn" Grid.Column="1" Width="80" Height="24" VerticalAlignment="Center" Click="OpenBtn_Click">
            Open
        </Button>
    </Grid>
</UserControl>
